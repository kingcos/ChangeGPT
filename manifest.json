{
  "name": "ChangeGPT",
  "description":"An extension for changing your ChatGPT avatar locally / 更换 ChatGPT 对话中你的头像",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_title": "ChangeGPT"
  },
  "permissions": ["activeTab", "storage"],
  "host_permissions": ["https://chat.openai.com/*"],
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["content.js"]
    }
  ],
	"icons": {
 	  "16": "/icons/changegpt-16.png",
	  "32": "/icons/changegpt-32.png",
	  "48": "/icons/changegpt-48.png",
	  "128": "/icons/changegpt-128.png"
	}
}
